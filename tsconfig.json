{
  "compilerOptions": {
    "checkJs": true,
    "noEmit": true,
    "lib": ["DOM", "ES6", "ES2020"],
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "module": "ES6",
    "moduleResolution": "node",
    "target": "ES6",
    "typeRoots": ["./src/types"],
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["./components/*"],
      "@helpers/*": ["./helpers/*"]
    }
  },
  "include": [".aha-cache/types", "src"]
}
